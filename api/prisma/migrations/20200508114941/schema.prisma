datasource DS {
  provider = "postgres"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Post {
  id         Int      @default(autoincrement()) @id
  title      String
  body       String
  createdAt  DateTime @default(now())
  metafields Json     @default("'{}'::jsonb")
}
